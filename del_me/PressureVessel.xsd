<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:PressureVessel" xmlns:cw="urn:PressureVessel" version="1.07.0074">
    <!--<?xml version="1.0" ?>-->
    <xsd:annotation>
        <xsd:documentation>
            Pressure Vessel and Heat Exchanger Schema
            XML Schema for Pressure Vessels and Heat Exchangers. Created by Codeware Inc.
