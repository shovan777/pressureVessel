    <!-- Root element - Main Vessel -->
    <xsd:element name="vessel">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:choice>
                    <!-- Allow for either a pressure vessel or a heat exchanger -->
                    <xsd:element name="pressureVessel" maxOccurs="1" type="cw:PressureVessel" />
                    <xsd:element name="heatExchanger" maxOccurs="1" type="cw:HeatExchanger" />
                </xsd:choice>
                <xsd:element name="codewareData" minOccurs="0" maxOccurs="1" type="cw:CodewareData" />
                <xsd:element name="formatSettings" minOccurs="0" maxOccurs="1" type="cw:FormatSettings" />
            </xsd:sequence>
            <xsd:attribute name="version" use="required" form="unqualified" type="xsd:string" />
            <xsd:attribute name="creationDate" use="optional" form="unqualified" type="xsd:date" />
        </xsd:complexType>
    </xsd:element>
    <!-- /////////////////////////////////////////////////////////// -->
    <!-- Complex Element Types -->
    <!-- Heat Exchanger Element -->
    <xsd:complexType name="HeatExchanger">
        <xsd:sequence>
            <xsd:element name="revisions" minOccurs="0" maxOccurs="1">
                <xsd:complexType>
                    <xsd:sequence>
                        <!-- Revision Element -->
                        <xsd:element name="revision" minOccurs="0" maxOccurs="unbounded" type="cw:Revision" />
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="engineeringNotes" minOccurs="0" maxOccurs="1" type="xsd:string" />
            <xsd:element name="generalVesselInfo" minOccurs="1" maxOccurs="1" type="cw:GeneralVesselInfo" />
            <xsd:element name="heatExchangerGeneralInfo" minOccurs="0" maxOccurs="1" type="cw:HeatExchangerGeneralInfo" />
            <xsd:element name="heatExchangerDesignConditions" minOccurs="0" maxOccurs="1" type="cw:HeatExchangerDesignConditions" />
            <xsd:element name="pressureChamberConditions" minOccurs="1" maxOccurs="unbounded" type="cw:PressureChamberConditions" />
            <xsd:element name="vesselResults" minOccurs="1" maxOccurs="1" type="cw:VesselResults" />
            <!-- Top / Left Closure Element -->
            <xsd:element name="closure1" minOccurs="0" maxOccurs="1">
                <xsd:complexType>
                    <xsd:choice>
                        <xsd:element name="topClosure" minOccurs="0" maxOccurs="1" type="cw:Closure" />
                        <xsd:element name="leftClosure" minOccurs="0" maxOccurs="1" type="cw:Closure" />
                    </xsd:choice>
                </xsd:complexType>
            </xsd:element>
            <!-- Bottom / Right Closure Element -->
            <xsd:element name="closure2" minOccurs="0" maxOccurs="1">
                <xsd:complexType>
                    <xsd:choice>
                        <xsd:element name="bottomClosure" minOccurs="0" maxOccurs="1" type="cw:Closure" />
                        <xsd:element name="rightClosure" minOccurs="0" maxOccurs="1" type="cw:Closure" />
                    </xsd:choice>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="frontChannel" minOccurs="1" maxOccurs="1" type="cw:HEChannel" />
            <xsd:element name="rearChannel" minOccurs="0" maxOccurs="1" type="cw:HEChannel" />
            <xsd:element name="innerHead" minOccurs="0" maxOccurs="1" type="cw:Closure" />
            <xsd:element name="frontTubesheet" minOccurs="1" maxOccurs="1" type="cw:Tubesheet" />
            <xsd:element name="rearTubesheet" minOccurs="0" maxOccurs="1" type="cw:Tubesheet" />
            <xsd:element name="shell" minOccurs="1" maxOccurs="1" type="cw:HEShell" />
            <!-- Support Element -->
            <xsd:element name="support" minOccurs="0" maxOccurs="1">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="saddle" minOccurs="0" maxOccurs="1" type="cw:SaddleData" />
                        <xsd:choice>
                            <xsd:element name="legs" minOccurs="0" maxOccurs="1" type="cw:LegData" />
                            <xsd:element name="skirt" minOccurs="0" maxOccurs="unbounded" type="cw:Skirt" />
                            <xsd:element name="lugs" minOccurs="0" maxOccurs="1" type="cw:LugData" />
                            <xsd:element name="saddles" minOccurs="0" maxOccurs="1" type="cw:SaddlesData" />
                         </xsd:choice>
                    </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <!-- Wind Code Element -->
            <xsd:element name="windCode" minOccurs="0" maxOccurs="1">
                <xsd:complexType>
                    <xsd:choice>
                        <xsd:element name="ASCE" minOccurs="0" maxOccurs="1" type="cw:ASCEWindCode" />
                        <xsd:element name="NBC" minOccurs="0" maxOccurs="1" type="cw:NBCWindCode" />
                        <xsd:element name="UBC" minOccurs="0" maxOccurs="1" type="cw:UBCWindCode" />
                        <xsd:element name="IBC" minOccurs="0" maxOccurs="1" type="cw:ASCEWindCode" />
                        <xsd:element name="User" minOccurs="0" maxOccurs="1" type="cw:UserWindCode" />
                    </xsd:choice>
                </xsd:complexType>
            </xsd:element>
            <!-- Seismic Code Element -->
            <xsd:element name="seismicCode" minOccurs="0" maxOccurs="1">
                <xsd:complexType>
                    <xsd:choice>
                        <xsd:element name="ASCE" minOccurs="0" maxOccurs="1" type="cw:ASCESeismicCode98" />
                        <xsd:element name="ASCE95or93GS" minOccurs="0" maxOccurs="1" type="cw:ASCESeismicCode95or93GS" />
                        <xsd:element name="ASCE93BM" minOccurs="0" maxOccurs="1" type="cw:ASCESeismicCode93BM" />
                        <xsd:element name="ASCE88" minOccurs="0" maxOccurs="1" type="cw:ASCESeismicCode88" />
                        <xsd:element name="IBC" minOccurs="0" maxOccurs="1" type="cw:ASCESeismicCode98" />
                        <xsd:element name="NBC" minOccurs="0" maxOccurs="1" type="cw:NBCSeismicCode" />
                        <xsd:element name="NBC05" minOccurs="0" maxOccurs="1" type="cw:NBC05SeismicCode" />
                        <xsd:element name="UBC" minOccurs="0" maxOccurs="1" type="cw:UBCSeismicCode" />
                        <xsd:element name="User" minOccurs="0" maxOccurs="1" type="cw:UserSeismicCode" />
                    </xsd:choice>
                </xsd:complexType>
            </xsd:element>
            <!-- Liquid Level Element -->
            <xsd:element name="liquidLevel" minOccurs="0" maxOccurs="unbounded" type="cw:LiquidLevel" />
            <!-- Lateral Force Element -->
            <xsd:element name="lateralForce" minOccurs="0" maxOccurs="unbounded" type="cw:LateralForce" />
            <!-- Vertical Force Element -->
            <xsd:element name="verticalForce" minOccurs="0" maxOccurs="unbounded" type="cw:VerticalForce" />
            <!-- Tray Element -->
            <xsd:element name="trayGroup" minOccurs="0" maxOccurs="unbounded" type="cw:Tray" />
            <!-- Packed Bed Element -->
            <xsd:element name="packedBed" minOccurs="0" maxOccurs="unbounded" type="cw:PackedBed" />
            <!-- Piping Element -->
            <xsd:element name="pipingDetail" minOccurs="0" maxOccurs="unbounded" type="cw:PipingDetail" />
            <xsd:element name="pipingUser" minOccurs="0" maxOccurs="unbounded" type="cw:PipingUser" />
            <!-- Lift Lug Element -->
            <xsd:element name="liftLug" minOccurs="0" maxOccurs="4" type="cw:LiftLug" />
        </xsd:sequence>
    </xsd:complexType>
    <!-- Pressure Vessel Element -->
    <xsd:complexType name="PressureVessel">
        <xsd:sequence>
            <xsd:element name="revisions" minOccurs="0" maxOccurs="1">
                <xsd:complexType>
                    <xsd:sequence>
                        <!-- Revision Element -->
                        <xsd:element name="revision" minOccurs="0" maxOccurs="unbounded" type="cw:Revision" />
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="engineeringNotes" minOccurs="0" maxOccurs="1" type="xsd:string" />
            <xsd:element name="generalVesselInfo" minOccurs="1" maxOccurs="1" type="cw:GeneralVesselInfo" />
            <xsd:element name="pressureChamberConditions" minOccurs="1" maxOccurs="unbounded" type="cw:PressureChamberConditions" />
            <xsd:element name="vesselResults" minOccurs="1" maxOccurs="1" type="cw:VesselResults" />
            <!-- Top / Left Closure Element -->
            <xsd:element name="closure1" minOccurs="0" maxOccurs="1">
                <xsd:complexType>
                    <xsd:choice>
                        <xsd:element name="topClosure" minOccurs="0" maxOccurs="1" type="cw:Closure" />
                        <xsd:element name="leftClosure" minOccurs="0" maxOccurs="1" type="cw:Closure" />
                    </xsd:choice>
                </xsd:complexType>
            </xsd:element>
            <!-- Bottom / Right Closure Element -->
            <xsd:element name="closure2" minOccurs="0" maxOccurs="1">
                <xsd:complexType>
                    <xsd:choice>
                        <xsd:element name="bottomClosure" minOccurs="0" maxOccurs="1" type="cw:Closure" />
                        <xsd:element name="rightClosure" minOccurs="0" maxOccurs="1" type="cw:Closure" />
                    </xsd:choice>
                </xsd:complexType>
            </xsd:element>
            <!-- Shell Element -->
            <xsd:element name="shell" minOccurs="0" maxOccurs="1">
                <xsd:complexType>
                    <xsd:sequence>
                        <!-- Cylinder Element -->
                        <xsd:element name="cylinder" minOccurs="0" maxOccurs="unbounded" type="cw:Cylinder" />
                        <!-- Transition Element -->
                        <xsd:element name="transition" minOccurs="0" maxOccurs="unbounded" type="cw:Transition" />
                        <!-- Skirt Element -->
                        <xsd:element name="skirt" minOccurs="0" maxOccurs="unbounded" type="cw:Skirt" />
                        <!-- Inner Head Element -->
                        <xsd:element name="innerHead" minOccurs="0" maxOccurs="unbounded" type="cw:Closure" />
                        <!-- Body Flange Element -->
                        <xsd:element name="flange" minOccurs="0" maxOccurs="unbounded" type="cw:Flange" />
                        <!-- B31.3 Pipe Element -->
                        <xsd:element name="b313Pipe" minOccurs="0" maxOccurs="unbounded" type="cw:B313Pipe" />
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
          <!-- Support Element -->
            <xsd:element name="support" minOccurs="0" maxOccurs="1">
                <xsd:complexType>
                    <xsd:sequence>
                    <xsd:element name="saddle" minOccurs="0" maxOccurs="1" type="cw:SaddleData" />
                    <xsd:choice>
                        <xsd:element name="legs" minOccurs="0" maxOccurs="1" type="cw:LegData" />
                        <xsd:element name="skirt" minOccurs="0" maxOccurs="unbounded" type="cw:Skirt" />
                        <xsd:element name="lugs" minOccurs="0" maxOccurs="1" type="cw:LugData" />
                        <xsd:element name="saddles" minOccurs="0" maxOccurs="1" type="cw:SaddlesData" />
                    </xsd:choice>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
          <!-- Wind Code Element -->
            <xsd:element name="windCode" minOccurs="0" maxOccurs="1">
                <xsd:complexType>
                    <xsd:choice>
                        <xsd:element name="ASCE" minOccurs="0" maxOccurs="1" type="cw:ASCEWindCode" />
                        <xsd:element name="NBC" minOccurs="0" maxOccurs="1" type="cw:NBCWindCode" />
                        <xsd:element name="UBC" minOccurs="0" maxOccurs="1" type="cw:UBCWindCode" />
                        <xsd:element name="IBC" minOccurs="0" maxOccurs="1" type="cw:ASCEWindCode" />
                        <xsd:element name="User" minOccurs="0" maxOccurs="1" type="cw:UserWindCode" />
                    </xsd:choice>
                </xsd:complexType>
            </xsd:element>
            <!-- Seismic Code Element -->
            <xsd:element name="seismicCode" minOccurs="0" maxOccurs="1">
                <xsd:complexType>
                    <xsd:choice>
                        <xsd:element name="ASCE" minOccurs="0" maxOccurs="1" type="cw:ASCESeismicCode98" />
                        <xsd:element name="ASCE95or93GS" minOccurs="0" maxOccurs="1" type="cw:ASCESeismicCode95or93GS" />
                        <xsd:element name="ASCE93BM" minOccurs="0" maxOccurs="1" type="cw:ASCESeismicCode93BM" />
                        <xsd:element name="ASCE88" minOccurs="0" maxOccurs="1" type="cw:ASCESeismicCode88" />
                        <xsd:element name="IBC" minOccurs="0" maxOccurs="1" type="cw:ASCESeismicCode98" />
                        <xsd:element name="NBC" minOccurs="0" maxOccurs="1" type="cw:NBCSeismicCode" />
                        <xsd:element name="NBC05" minOccurs="0" maxOccurs="1" type="cw:NBC05SeismicCode" />
                        <xsd:element name="UBC" minOccurs="0" maxOccurs="1" type="cw:UBCSeismicCode" />
                        <xsd:element name="User" minOccurs="0" maxOccurs="1" type="cw:UserSeismicCode" />
                    </xsd:choice>
                </xsd:complexType>
            </xsd:element>
            <!-- Liquid Level Element -->
            <xsd:element name="liquidLevel" minOccurs="0" maxOccurs="unbounded" type="cw:LiquidLevel" />
            <!-- Lateral Force Element -->
            <xsd:element name="lateralForce" minOccurs="0" maxOccurs="unbounded" type="cw:LateralForce" />
            <!-- Vertical Force Element -->
            <xsd:element name="verticalForce" minOccurs="0" maxOccurs="unbounded" type="cw:VerticalForce" />
            <!-- Tray Element -->
            <xsd:element name="trayGroup" minOccurs="0" maxOccurs="unbounded" type="cw:Tray" />
            <!-- Packed Bed Element -->
            <xsd:element name="packedBed" minOccurs="0" maxOccurs="unbounded" type="cw:PackedBed" />
            <!-- Piping Element -->
            <xsd:element name="pipingDetail" minOccurs="0" maxOccurs="unbounded" type="cw:PipingDetail" />
            <xsd:element name="pipingUser" minOccurs="0" maxOccurs="unbounded" type="cw:PipingUser" />
            <!-- Lift Lug Element -->
            <xsd:element name="liftLug" minOccurs="0" maxOccurs="4" type="cw:LiftLug" />



          <!-- API 579 Flaw -->
          <xsd:element name="api579Flaws" minOccurs="0" maxOccurs="1">
            <xsd:complexType>
              <xsd:sequence>
                <!-- API 579 Metal Loss Element -->
                <xsd:element name="api579MetalLoss" minOccurs="0" maxOccurs="unbounded" type="cw:API579MetalLoss" />
                <!-- API 579 Pitting Element -->
                <xsd:element name="api579Pitting" minOccurs="0" maxOccurs="unbounded" type="cw:API579Pitting" />
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>

          <!-- Information Pertaining to All Inspections -->
          <xsd:element name="InspectionInfo" minOccurs="0" maxOccurs="1">
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element name="PSM" type="xsd:string" minOccurs="1" maxOccurs="1" />
                <xsd:element name="TypeOfConstruction" type="xsd:string" minOccurs="1" maxOccurs="1" />
                <xsd:element name="Contents" type="xsd:string" minOccurs="1" maxOccurs="1" />
                <xsd:element name="Process" type="xsd:string" minOccurs="1" maxOccurs="1" />
                <xsd:element name="RetirementDate" type="xsd:string" minOccurs="1" maxOccurs="1" />
                <xsd:element name="ConstructionDate" type="xsd:string" minOccurs="1" maxOccurs="1" />
                <xsd:element name="PID" type="xsd:string" minOccurs="1" maxOccurs="1" />
                <xsd:element name="OtherInfo" type="xsd:string" minOccurs="1" maxOccurs="1" />
                <xsd:element name="SerialNo" type="xsd:string" minOccurs="1" maxOccurs="1" />
                <xsd:element name="NatlBoardNo" type="xsd:string" minOccurs="1" maxOccurs="1" />
                <xsd:element name="CRN" type="xsd:string" minOccurs="1" maxOccurs="1" />
                <xsd:element name="EquipmentID" type="xsd:string" minOccurs="1" maxOccurs="1" />
                <xsd:element name="Manufacturer" type="xsd:string" minOccurs="1" maxOccurs="1" />
                <xsd:element name="Note" minOccurs="0" maxOccurs="unbounded" >
                  <xsd:complexType>
                    <xsd:sequence>
                      <xsd:element name="Label" minOccurs="0" maxOccurs="1" type="xsd:string" />
                      <xsd:element name="Text" minOccurs="0" maxOccurs="1" type="xsd:string" />
                    </xsd:sequence>
                  </xsd:complexType>
                </xsd:element>
                <xsd:element name="RelatedFiles" minOccurs="0" maxOccurs="unbounded" >
                  <xsd:complexType>
                    <xsd:sequence>
                      <xsd:element name="Label" minOccurs="0" maxOccurs="1" type="xsd:string" />
                      <xsd:element name="OriginalFilePath" minOccurs="0" maxOccurs="1" type="xsd:string" />
                      <xsd:element name="Embedded" minOccurs="0" maxOccurs="1" type="cw:Boolean" />
                    </xsd:sequence>
                  </xsd:complexType>
                </xsd:element>
                <xsd:element name="Inspection" minOccurs="0" maxOccurs="unbounded" >
                  <xsd:complexType>
                    <xsd:sequence>
                      <xsd:element name="InspectionDate" minOccurs="0" maxOccurs="1" type="xsd:string" />
                      <xsd:element name="InspectorName" minOccurs="0" maxOccurs="1" type="xsd:string" />
                      <xsd:element name="Description" minOccurs="0" maxOccurs="1" type="xsd:string" />
                      <xsd:element name="Comments" minOccurs="0" maxOccurs="1" type="xsd:string" />
                      <xsd:element name="Note" minOccurs="0" maxOccurs="unbounded" >
                        <xsd:complexType>
                          <xsd:sequence>
                            <xsd:element name="Label" minOccurs="0" maxOccurs="1" type="xsd:string" />
                            <xsd:element name="Text" minOccurs="0" maxOccurs="1" type="xsd:string" />
                          </xsd:sequence>
                        </xsd:complexType>
                      </xsd:element>
                      <xsd:element name="RelatedFiles" minOccurs="0" maxOccurs="unbounded" >
                        <xsd:complexType>
                          <xsd:sequence>
                            <xsd:element name="Label" minOccurs="0" maxOccurs="1" type="xsd:string" />
                            <xsd:element name="OriginalFilePath" minOccurs="0" maxOccurs="1" type="xsd:string" />
                            <xsd:element name="Embedded" minOccurs="0" maxOccurs="1" type="cw:Boolean" />
                          </xsd:sequence>
                        </xsd:complexType>
                      </xsd:element>
                      <xsd:element name="FlangedJoint" minOccurs="0" maxOccurs="unbounded" >
                        <xsd:complexType>
                          <xsd:sequence>
                            <xsd:element name="identifier" minOccurs="0" maxOccurs="1" type="xsd:string" />
                            <xsd:element name="idNumber" minOccurs="0" maxOccurs="1" type="xsd:integer" />
                            <xsd:element name="EvidenceOfFlangeLeakage" maxOccurs="1" type="cw:Boolean" />
                            <xsd:element name="FlangeLeakageDescription" maxOccurs="1" type="xsd:string" />
                            <xsd:element name="FlangeFaceAccessible" maxOccurs="1" type="cw:Boolean" />
                            <xsd:element name="FlangeFaceInspected" maxOccurs="1" type="cw:Boolean" />
                            <xsd:element name="FlangeFaceCondition" maxOccurs="1" type="xsd:string" />
                            <xsd:element name="BoltCorrosionPresent" maxOccurs="1" type="cw:Boolean" />
                            <xsd:element name="BoltAcceptableThreadEngagement" maxOccurs="1" type="cw:Boolean" />
                            <xsd:element name="BoltingComments" maxOccurs="1" type="xsd:string" />
                            <xsd:element name="NewFastenersMarkingsChecked" maxOccurs="1" type="cw:Boolean" />
                            <xsd:element name="NewGasketMarkingsChecked" maxOccurs="1" type="cw:Boolean" />
                            <xsd:element name="MarkingsComments" maxOccurs="1" type="xsd:string" />
                          </xsd:sequence>
                        </xsd:complexType>
                      </xsd:element>
                      <xsd:element name="InspectionPoint" minOccurs="0" maxOccurs="unbounded" >
                        <xsd:complexType>
                          <xsd:sequence>
                            <xsd:element name="identifier" maxOccurs="1" type="xsd:string" />
                            <xsd:element name="idNumber" maxOccurs="1" type="xsd:integer" />
                            <xsd:element name="attachedTo" maxOccurs="1" type="cw:AttachedTo" />
                            <xsd:element name="attachedToKind" maxOccurs="1" type="cw:ComponentKind" />
                            <xsd:element name="attachedToidNumber" maxOccurs="1" type="xsd:integer" />
                            <xsd:element name="orientationAngle" maxOccurs="1" type="cw:decimalUnits" />
                            <xsd:element name="distanceFromHeadCenter" minOccurs="0" maxOccurs="unbounded" type="cw:decimalUnits" />
                            <xsd:element name="distanceFromNozzleBase" minOccurs="0" maxOccurs="1" type="cw:decimalUnits" />
                            <xsd:element name="distanceToDatum" maxOccurs="1" type="cw:decimalUnits" />
                            <xsd:element name="Commentary" maxOccurs="1" type="xsd:string" />
                            <xsd:element name="measuredThickness" maxOccurs="1" type="cw:zeroOrGreaterUnits" />
                            <xsd:element name="lossOfMetal" minOccurs="0" maxOccurs="1" type="cw:decimalUnits" />
                            <xsd:element name="longTermCorrosionRate" minOccurs="0" maxOccurs="1" type="cw:decimalUnits" />
                            <xsd:element name="shortTermCorrosionRate" minOccurs="0" maxOccurs="1" type="cw:decimalUnits" />
                            <xsd:element name="yearsRemainingUntilMinimumThickness" minOccurs="0" maxOccurs="1" type="cw:decimalUnits" />
                            <xsd:element name="MaxTemperature" maxOccurs="1" type="cw:decimalUnits" />
                            <xsd:element name="jointEfficiency" maxOccurs="1" type="cw:zeroOrGreater" />
                            <xsd:element name="Note" minOccurs="0" maxOccurs="unbounded" >
                              <xsd:complexType>
                                <xsd:sequence>
                                  <xsd:element name="Label" minOccurs="0" maxOccurs="1" type="xsd:string" />
                                  <xsd:element name="Text" minOccurs="0" maxOccurs="1" type="xsd:string" />
                                </xsd:sequence>
                              </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="RelatedFiles" minOccurs="0" maxOccurs="unbounded" >
                              <xsd:complexType>
                                <xsd:sequence>
                                  <xsd:element name="Label" minOccurs="0" maxOccurs="1" type="xsd:string" />
                                  <xsd:element name="OriginalFilePath" minOccurs="0" maxOccurs="1" type="xsd:string" />
                                  <xsd:element name="Embedded" minOccurs="0" maxOccurs="1" type="cw:Boolean" />
                                </xsd:sequence>
                              </xsd:complexType>
                            </xsd:element>
                          </xsd:sequence>
                        </xsd:complexType>
                      </xsd:element>
                    </xsd:sequence>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>


        </xsd:sequence>
    </xsd:complexType>
    <!-- End of Pressure Vessel Element -->
    <!-- Revisions Info Element -->
    <xsd:complexType name="Revision">
        <xsd:sequence>
            <xsd:element name="revNumber" type="xsd:integer" minOccurs="1" maxOccurs="1" />
            <xsd:element name="revDate" form="unqualified" type="xsd:date" minOccurs="0" maxOccurs="1" />
            <xsd:element name="operator" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="notes" type="xsd:string" minOccurs="0" maxOccurs="1" />
        </xsd:sequence>
    </xsd:complexType>
    <!-- General Vessel Info Element -->
    <xsd:complexType name="GeneralVesselInfo">
        <xsd:sequence>
            <xsd:element name="identifier" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="location" type="xsd:string" minOccurs="0" maxOccurs="1" />
