    <!-- NBC Seismic Code Data Element -->
    <xsd:complexType name="NBCSeismicCode">
        <xsd:sequence>
            <xsd:element name="edition" type="xsd:string" minOccurs="1" maxOccurs="1" />
            <xsd:element name="zonalVelocityRatio-v" type="cw:zeroOrGreater" minOccurs="0" maxOccurs="1" />
            <xsd:element name="importanceFactor-I" type="cw:zeroOrGreater" minOccurs="0" maxOccurs="1" />
            <xsd:element name="accelerationCoefficient-Za" type="cw:zeroOrGreater" minOccurs="0" maxOccurs="1" />
            <xsd:element name="velocityCoefficient-Zv" type="cw:zeroOrGreater" minOccurs="0" maxOccurs="1" />
            <xsd:element name="foundationFactor-F" type="cw:zeroOrGreater" minOccurs="0" maxOccurs="1" />
            <xsd:element name="forceModificationFactor-R" type="cw:zeroOrGreater" minOccurs="0" maxOccurs="1" />
            <!-- Saddle Seismic  -->
            <xsd:element name="saddleSeisStructureHeight-hn" type="cw:zeroOrGreaterUnits" minOccurs="0" maxOccurs="1" />
            <xsd:element name="saddleSeisVesselElev-hx" type="cw:zeroOrGreaterUnits" minOccurs="0" maxOccurs="1" />
            <xsd:element name="saddleSeisResponseFactor-Ar" type="cw:zeroOrGreater" minOccurs="0" maxOccurs="1" />
            <!-- Vertical Accel.  -->
            <xsd:element name="includeVerticalAcceleration" type="cw:Boolean" default="TRUE" minOccurs="1" maxOccurs="1" />
            <xsd:element name="forceMultiplier" type="cw:zeroOrGreater" default="1.0" minOccurs="1" maxOccurs="1" />
            <xsd:element name="minimumWeightMultiplier" type="cw:zeroOrGreater" default="1.0" minOccurs="1" maxOccurs="1" />
            <!-- Results -->
            <xsd:element name="fundamentalPeriodOperating" type="cw:zeroOrGreater" minOccurs="0" maxOccurs="1" />
            <xsd:element name="fundamentalPeriodEmpty" type="cw:zeroOrGreater" minOccurs="0" maxOccurs="1" />
        </xsd:sequence>
    </xsd:complexType>
    <!-- UBC Seismic Code Data Element -->
    <xsd:complexType name="UBCSeismicCode">
        <xsd:sequence>
            <xsd:element name="edition" type="xsd:string" minOccurs="1" maxOccurs="1" />
            <xsd:element name="zone" type="cw:UBCZone" minOccurs="0" maxOccurs="1" />
            <xsd:element name="importanceFactor-I" type="cw:zeroOrGreater" minOccurs="0" maxOccurs="1" />
            <xsd:choice>
                <xsd:element name="soilProfile97" type="cw:UBCSoilProfile1997" minOccurs="0" maxOccurs="1" />
                <xsd:element name="soilProfilePre97" type="cw:UBCSoilProfilePre1997" minOccurs="0" maxOccurs="1" />
            </xsd:choice>
            <xsd:element name="RFactor" type="cw:zeroOrGreater" minOccurs="0" maxOccurs="1" />
            <xsd:element name="hxOverhrRatio" type="cw:zeroOrGreater" minOccurs="0" maxOccurs="1" />
            <xsd:element name="amplificationFactor-ap" type="cw:zeroOrGreater" default="1.0" minOccurs="0" maxOccurs="1" />
            <!-- For Pre 1997 code - need to add a conditional 'if' for this -->
            <xsd:element name="siteCoefficient-S" type="cw:zeroOrGreater" default="1.0" minOccurs="0" maxOccurs="1" />
            <xsd:element name="coefficient-Rw" type="cw:zeroOrGreater" default="1.0" minOccurs="0" maxOccurs="1" />
            <!-- Vertical Accel.  -->
            <xsd:element name="includeVerticalAcceleration" type="cw:Boolean" default="TRUE" minOccurs="1" maxOccurs="1" />
            <xsd:element name="forceMultiplier" type="cw:zeroOrGreater" default="1.0" minOccurs="1" maxOccurs="1" />
            <xsd:element name="minimumWeightMultiplier" type="cw:zeroOrGreater" default="1.0" minOccurs="1" maxOccurs="1" />
            <!-- Results -->
            <xsd:element name="fundamentalPeriodOperating" type="cw:zeroOrGreater" minOccurs="0" maxOccurs="1" />
            <xsd:element name="fundamentalPeriodEmpty" type="cw:zeroOrGreater" minOccurs="0" maxOccurs="1" />
        </xsd:sequence>
    </xsd:complexType>
    <!-- User Seismic Code Data Element -->
    <xsd:complexType name="UserSeismicCode">
        <xsd:sequence>
            <xsd:element name="baseShearMultiplier" type="cw:zeroOrGreater" minOccurs="1" maxOccurs="1" />
            <xsd:element name="portionAtTop" type="cw:zeroOrGreater" minOccurs="1" maxOccurs="1" />
            <!-- Vertical Accel.  -->
            <xsd:element name="includeVerticalAcceleration" type="cw:Boolean" default="TRUE" minOccurs="1" maxOccurs="1" />
            <xsd:element name="forceMultiplier" type="cw:zeroOrGreater" default="1.0" minOccurs="1" maxOccurs="1" />
            <xsd:element name="minimumWeightMultiplier" type="cw:zeroOrGreater" default="1.0" minOccurs="1" maxOccurs="1" />
            <!-- Results -->
            <xsd:element name="fundamentalPeriodOperating" type="cw:zeroOrGreater" minOccurs="0" maxOccurs="1" />
            <xsd:element name="fundamentalPeriodEmpty" type="cw:zeroOrGreater" minOccurs="0" maxOccurs="1" />
        </xsd:sequence>
    </xsd:complexType>
